    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 28,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.4,
    "spacing_after": 13.01,
    "page": 4
  },
  {
    "text": "Guidance and Advice:",
    "font_size": 11.039999961853027,
    "bold": 1,
    "length": 20,
    "is_upper": 0,
    "indent": 90.0,
    "spacing_before": 13.01,
    "spacing_after": 0.28,
    "page": 4
  },
  {
    "text": "to support e-learning",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 21,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.28,
    "spacing_after": 0.4,
    "page": 4
  },
  {
    "text": "to support citizens, in real-time, as they try to use electronic resources",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 74,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.4,
    "spacing_after": 0.28,
    "page": 4
  },
  {
    "text": "to assist citizens as they use web-links",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 40,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.28,
    "spacing_after": 0.28,
    "page": 4
  },
  {
    "text": "Training:",
