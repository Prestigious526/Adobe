    "font_size": 11.039999961853027,
    "bold": 1,
    "length": 7,
    "is_upper": 0,
    "indent": 90.0,
    "spacing_before": 13.01,
    "spacing_after": 0.4,
    "page": 4
  },
  {
    "text": "to the \u201cdeep\u201d web (i.e. Commercial, purchased electronic services)",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 66,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.4,
    "spacing_after": 0.28,
    "page": 4
  },
  {
    "text": "to credible web sites and electronic resources",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 46,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.28,
    "spacing_after": 0.28,
    "page": 4
  },
  {
    "text": "to digital government information",
    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 33,
    "is_upper": 0,
    "indent": 107.99,
    "spacing_before": 0.28,
    "spacing_after": 0.4,
    "page": 4
  },
  {
