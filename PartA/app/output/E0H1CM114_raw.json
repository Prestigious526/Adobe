    "font_size": 11.039999961853027,
    "bold": 0,
    "length": 29,
    "is_upper": 0,
    "indent": 108.03,
    "spacing_before": 0.4,
    "spacing_after": 12.28,
    "page": 3,
    "line_top": 423.6670227050781,
    "line_bottom": 435.9876708984375
  },
  {
    "text": "Shared decision-making and accountability:",
    "font_size": 11.039999961853027,
    "bold": 1,
    "length": 42,
    "is_upper": 0,
    "indent": 90.03,
    "spacing_before": 12.28,
    "spacing_after": 12.4,
    "page": 3,
    "line_top": 448.2641296386719,
    "line_bottom": 460.58477783203125
  },
